#include <iostream>
#include <cmath>
using namespace std;

int main() {
  cout << exp(0) << "\n";
  cout << exp(1) << "\n";
  cout << exp(10) << "\n";
  cout << exp(4.8) << "\n";
  cout << exp(2.718) << "\n";
  cout << floor(0.60);
  cout << floor(0.40);
  cout << floor(5);
  cout << floor(5.1);
  cout << floor(-5.1);
  cout << floor(-5.9);
  return 0;
}
