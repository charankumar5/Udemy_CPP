#include <iostream>
#include <cmath>
using namespace std;

// Ref: https://www.w3schools.com/cpp/cpp_ref_math.asp
int main() {
  cout << exp(0) << endl;
  cout << exp(1) <<  endl;
  cout << exp(10) << endl;
  cout << exp(4.8) << endl;
  cout << exp(2.718) <<  endl;
  cout << floor(0.60) << endl;
  cout << floor(0.40)<< endl;
  cout << floor(5)<< endl;
  cout << floor(5.1)<< endl;
  cout << floor(-5.1)<< endl;
  cout << floor(-5.9)<< endl;
  cout << log(6.0f)<< endl;
  cout << log(2.0)<< endl;
  cout << log(1.0)<< endl;
  cout << log(0.0)<< endl;
  cout << log(-1.0f)<< endl;
  return 0;
}
