    // TODO: Add your program code here
	cout << "Day3_Operations started." << endl << endl;
	//postfix and prefix
	int a {10};
	cout << "a :"  << a <<" "<< ++a <<" " << a++ << endl;
	cout << "a: " << a << " "<< --a <<" "<< a << endl;

	bool x {true};

	cout << x << endl;
	cout << !x << endl;

	return 0;
}
